
    <div>
        <div class="encabezadoTitulo">
            <h1 class="tituloRealtime">Vape Empire</h1>
            <h2 class="subtituloRealtime">Todos nuestros productos en venta</h2>
        </div>
    <form id="productForm" method="POST" action="/realtimeproducts">
    <label class="nombreFormulario" for="productName">Nombre del producto:</label>
    <ul id="productList">
        {{#each products}}
            <li>
                <div class="divImage">
                    <img class="productImg" src={{this.thumbnail}} />
                </div>
                <div class="tarjetaProducto">
                    <h3>{{this.name}}</h3>
                    <p class="tituloTarjeta">{{this.title}}</p>
                    <p>{{this.description}}</p>
                    <p class="precioTarjeta">Precio: {{this.price}}</p>
                </div>
            </li>
        {{/each}}
    </ul>
</div>
<script src="/js/index.js"></script>
<script src="/js/realTimeProducts.js"></script




