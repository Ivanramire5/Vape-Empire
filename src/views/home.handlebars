
<div>
    <h1>Lista de nuestros productos</h1>
    <ul>
        {{#each products}}
        <li>
            <div class="datos">
                <h2>{{title}}</h2>
                <div class="descripcionProductos">
                    <p>Description: {{this.description}}</p>
                    <p>Category: {{this.category}}</p>
                </div>
                <div class="preciosProducts">
                    <p>Price: {{this.price}}</p>
                    <p>Stock: {{this.stock}}</p>
                </div>
            </div>
            <img src={{this.thumbnails}} alt={{this.title}}>
        </li>
        {{/each}}
    </ul>
</div>